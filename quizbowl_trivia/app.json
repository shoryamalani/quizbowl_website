{
  "expo": {
    "name": "Trivia Slam",
    "slug": "quizbowl_trivia",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./app/assets/questionMarkIcon.png",
    
    "splash":{
      "image": "./app/assets/questionMarkIcon.png",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clearpointmanagement.quizbowl-trivia",
      "config":{
        "usesNonExemptEncryption":false
      }
    },
    "android": {},
    "web": {}
  }
}
