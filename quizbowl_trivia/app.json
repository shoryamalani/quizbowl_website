{
  "expo": {
    "name": "Trivia Slam",
    "displayName": "Trivia Slam",
    "slug": "quizbowl_trivia",
    "privacy": "unlisted",
    "version": "2.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./app/assets/questionMarkIcon.png",
    "splash": {
      "image": "./app/assets/questionMarkIcon.png",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./app/assets/questionMarkIcon.png",
      "bundleIdentifier": "com.clearpointmanagement.quizbowl_trivia.dev",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.smalani.quizbowl_trivia"
    },
    "web": {}
  }
}
